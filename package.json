{
  "name": "openclaw-groupme",
  "version": "0.0.4",
  "description": "OpenClaw GroupMe channel plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/oddrationale/openclaw-groupme"
  },
  "homepage": "https://github.com/oddrationale/openclaw-groupme#readme",
  "bugs": {
    "url": "https://github.com/oddrationale/openclaw-groupme/issues"
  },
  "type": "module",
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "openclaw": ">=2026.2.14",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "openclaw": ">=2026.2.14"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    }
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "groupme",
      "label": "GroupMe",
      "selectionLabel": "GroupMe (Bot API)",
      "blurb": "GroupMe bot webhook integration (group chats only)."
    },
    "install": {
      "npmSpec": "openclaw-groupme"
    }
  }
}
